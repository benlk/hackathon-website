<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "hack.osu" %></title>
    <%= stylesheet_link_tag "all", "foundation" %>
    <%= javascript_include_tag  "vendor/modernizr" %>
    <!-- Favicon -->
    <link rel="icon" href="images/favicon.ico" type="image/x-icon" />
 </head>

  <body class="<%= page_classes %>">
    <div id="not-footer">
      <div class="contain-to-grid" id="nav-parent">
        <nav class="top-bar" data-topbar>
          <ul class="title-area">
            <li class="name">
              <h1><%= link_to 'hack.osu', '/index.html' %></h1>
            </li>
            <li class="toggle-topbar menu-icon"><a href="#">Menu</a></li>
          </ul>

          <section class="top-bar-section">
            <!-- Right Nav Section -->
            <ul class="right">
              <li class=""><a href="#">#</a></li>
            </ul>
          </section>
        </nav>
      </div>
      <%= yield %>
    </div>
    <footer>
      <div class="row">
        <div class="small-12 medium-10 large-10 columns">
          <ul class="inline-list">
            <li><a href="http://opensource.osu.edu"><img src="images/footer-osc-white.png" height="30px"width ="30px" id="footer-logo">Open Source Club</a></li>
            <li><a href="https://opensource.osu.edu/about">About</a></li>
            <li><a href="https://opensource.osu.edu/contact">Contact</a></li>
            <li><a href="https://opensource.osu.edu/club_meetings">Join</a></li>
          </ul>
          
        </div>
        <div class="small-12 medium-2 large-2 columns" id="logo">
          <a href="https://creativecommons.org/licenses/by-sa/4.0/"><img src="images/footer-by-sa.png" alt="Creative Commons Attribution-Share Alike 4.0"></a>
        <div>
      </div>
    </footer>
    <%= javascript_include_tag  "all" %>
  </body>
</html>
